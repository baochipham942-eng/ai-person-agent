{
  "name": "ai-person-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4001",
    "dev:inngest": "npx inngest-cli@latest dev -u http://localhost:4001/api/inngest",
    "dev:all": "concurrently \"pnpm dev\" \"sleep 3 && pnpm dev:inngest\"",
    "build": "prisma generate && next build",
    "start": "next start -p 4001",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "deploy:build": "prisma generate && next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && cp bootstrap .next/standalone/"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^2.0.1",
    "@ai-sdk/openai": "^3.0.1",
    "@arco-design/web-react": "^2.66.8",
    "@neondatabase/serverless": "^0.10.4",
    "@prisma/adapter-neon": "^5.22.0",
    "@prisma/client": "5.22.0",
    "ai": "^6.0.3",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "exa-js": "^2.0.12",
    "inngest": "^3.48.1",
    "next": "16.1.1",
    "next-auth": "5.0.0-beta.30",
    "openai": "^6.15.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "ws": "^8.18.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sharp": "^0.32.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prisma": "5.22.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}